<div class="row justify-content-center">
  <div class="col-md-10">
    <div class="edit-review-form">
      <h1>Edit Review</h1>
      <%= simple_form_for ([@movie, @review]) do |f| %>
        <div id="rating-form">
          <%= f.input :rating, collection: 1..5 %>
        </div>
        <%= f.input :comment %>
        <%= f.button :submit, class:"btn btn-default" %>
      <% end %>
    </div>
  </div>
</div>
